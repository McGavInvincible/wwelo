div
    &.past-event-match
        padding: 10px
        margin: 10px
        display: flex
        flex-direction: column
        justify-content: space-around
        border: lightgrey 1px solid
        width: 800px
        border-radius: 4px
        align-self: center

    &.past-event-teams
        padding: 10px
        margin: 10px
        display: flex
        flex-wrap: wrap
        justify-content: space-around
        width: 800px
        align-self: center

    &.past-event-team
        padding: 10px
        margin: 10px

    &.past-event-elo-change-win
        color: green
        display: inline

    &.past-event-elo-change-loss
        color: red
        display: inline
