div
    &.title-holder-and-contenders
        padding: 10px
        margin: 10px
        display: flex
        justify-content: space-around
        border: lightgrey 1px solid
        width: 800px
        border-radius: 4px
        align-self: center

    &.content-title
        border: none 
        padding-bottom: 0px

    &.contenders
        width: 350px
        display: flex
        flex-direction: column

    &.wrestler
        display: flex
        justify-content: space-between

    &.contender
        width: 250px
        display: flex
        justify-content: space-between

.title-holder
        width: 250px
        display: flex
        flex-direction: column
        justify-content: space-between

h2
    font-weight: bold
    font-size: 16px